@model Ogretmen
<h2>Öğrenci</h2>
<hr>
<form asp-controller = "Ogretmen" asp-action = "create" method="post">
    <div class="mb-3">
        <label asp-for="Ad" class="form-label">Öğretmen Adı</label>
        <input asp-for = "Ad" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Soyad" class="form-label">Öğretmen Soyad</label>
        <input asp-for = "Soyad" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Telefon" class="form-label">Öğrenci Telefon</label>
        <input asp-for = "Telefon" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="BaslamaTarihi" class="form-label">BaslamaTarihi</label>
        <input asp-for = "BaslamaTarihi" class="form-control">
    </div>
    <button type="submit" class="btn btn-secondary">Kaydet</button>
</form>